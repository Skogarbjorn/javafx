<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="459.0" prefWidth="429.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.audioplayer.PlayerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <VBox alignment="CENTER" prefHeight="344.0" prefWidth="390.0">
        <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                <children>

                    <Label fx:id="welcomeText" text="Hello: " />
                    <Label fx:id="InitialCredentials" text="Guest" />
                </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#onLoginButtonClick" text="Skra inn" />
            <GridPane alignment="BOTTOM_CENTER" hgap="5.0" prefHeight="139.0" prefWidth="390.0" vgap="5.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onSelectList" text="J-rock" GridPane.columnIndex="0" />
                    <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onSelectList" text="Jpop" GridPane.columnIndex="1" />
                </children>
                <VBox.margin>
                    <Insets top="30.0" />
                </VBox.margin>
            </GridPane>
        </children>
    </VBox>
</VBox>
